{
	"name": "@thalessr/sync-resellers",
	"description": "Extension that syncs the resellers from the reseller API to the Directus database",
	"author": "Directus",
	"license": "MIT",
	"icon": "extension",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/thalessr/directus-sync-resellers.git"
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-hook"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.0.0"
	}
}
